{
  "name": "react-sample",
  "version": "1.0.0",
  "dependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.1.1",
    "effector": "^20.14.0",
    "effector-react": "^20.7.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "reshadow": "^0.0.1-alpha.76"
  },
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write **/*.{html,css,js*,md}",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "prettier": {
    "plugins": [
      "reshadow/prettier"
    ],
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}
